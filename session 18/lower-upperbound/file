#include<bits/stdc++.h>
using namespace std;
int lowerbound(int arr[], int n, int target) {
    int l=0;
    int r=n-1;
    int ans=n;
    while(l<=r){
        int mid = l+(r-l)/2;
        if(arr[mid] >= target) {
            ans=mid;
            r=mid-1;
        }
        else l=mid+1;
    }
    return ans;
}
int upperbound(int arr[], int n, int target) {
    int l=0;
    int r=n-1;
    int ans=-1;
    while(l<=r){
        int mid = l+(r-l)/2;
        if(arr[mid] > target) {
            ans=mid;
            r=mid-1;
        }
        else l=mid+1;
    }
    return ans;
}
int main(){
    int arr[]={1,2,3,4,5,6,7};
    int n= sizeof(arr)/sizeof(arr[0]);
cout<<lowerbound(arr,n,5)<<endl;
cout<<upperbound(arr,n,5)<<endl;
return 0;
}
